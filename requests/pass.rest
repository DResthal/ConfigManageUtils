# Return ALL params stored in params table
POST http://localhost:5000/getparams HTTP/1.1
Authorization: Basic dXNlcm5hbWU=:cGFzc3dvcmQ=
content-type: Application/json

{
  "userInfo": {
    "userName": "nclack",
    "userEmail": "testing@gmail.com"
  }
}

